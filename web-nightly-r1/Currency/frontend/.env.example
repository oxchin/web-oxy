# Production Environment Configuration
# Kconvert Currency Converter - LTS Optimized

# API Configuration - Safe for browser exposure
API_BASE_URL=https://xyz.io
APP_NAME=Kconvert
APP_VERSION=Stellar-1.5

# Build Configuration - Internal use only (not exposed to browser)
BUILD_MODE=development
ENABLE_ANALYTICS=true
ENABLE_PWA=true

# Performance Settings - Internal use only (not exposed to browser)
CACHE_DURATION=300000
REQUEST_TIMEOUT=10000
RETRY_ATTEMPTS=3

# Security Settings - Internal use only (not exposed to browser)
CSP_ENABLED=true
SECURE_HEADERS=true
